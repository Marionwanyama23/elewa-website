{
  "name": "models-schema-ui-cards",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/models/schema/ui/cards/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/models/schema/ui/cards",
        "main": "libs/models/schema/ui/cards/src/index.ts",
        "tsConfig": "libs/models/schema/ui/cards/tsconfig.lib.json",
        "assets": ["libs/models/schema/ui/cards/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/models/schema/ui/cards/**/*.ts",
          "libs/models/schema/ui/cards/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/models/schema/ui/cards/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
